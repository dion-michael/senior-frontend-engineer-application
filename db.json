    {
      "id": "form_001",
      "form_name": "General Health Anamnesis",
      "description": "Collects general health information from the patient.",
      "sections": [
        {
          "id": "section_001",
          "section_name": "Personal Information",
          "questions": [
            {
              "id": "question_001",
              "question_type": "text",
              "label": "Full Name",
              "placeholder": "Enter your full name",
              "required": true
            },
            {
              "id": "question_002",
              "question_type": "date",
              "label": "Date of Birth",
              "placeholder": "Select your date of birth",
              "required": true
            },
            {
              "id": "question_003",
              "question_type": "radio",
              "label": "Gender",
              "required": true,
              "options": [
                { "id": "opt_001", "label": "Male", "value": "male" },
                { "id": "opt_002", "label": "Female", "value": "female" },
                { "id": "opt_003", "label": "Other", "value": "other" }
              ]
            }
          ]
        },
        {
          "id": "section_002",
          "section_name": "Medical History",
          "questions": [
            {
              "id": "question_004",
              "question_type": "checkbox",
              "label": "Chronic Diseases",
              "options": [
                { "id": "opt_004", "label": "Diabetes", "value": "diabetes" },
                {
                  "id": "opt_005",
                  "label": "Hypertension",
                  "value": "hypertension"
                },
                {
                  "id": "opt_006",
                  "label": "Heart Disease",
                  "value": "heart_disease"
                }
              ]
            }
          ]
        }
      ],
      "created_at": "2024-08-14",
      "updated_at": "2024-08-14"
    },
